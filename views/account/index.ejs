<!-- Content Heading -->
<% if (title) { %>
    <h1><%= title %></h1>
<% } else { res.redirect('/') } %>

<!-- Messages -->
<%- messages() %>

<!-- Errors -->
<% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error=> { %>
        <li><%= error.msg %></li>
        <% }) %>
    </ul>
<% } %>

<!-- Account Management view to display -->
<div class="view-container">
    <p id="account-management-message">You're logged in</p>
</div>